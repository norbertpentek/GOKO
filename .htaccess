<IfModule mod_expires.c>
    ExpiresActive On
  
    # CSS fájl gyorsítótárazása 1 hónapig
    ExpiresByType text/css "access plus 1 month"
  
    # JavaScript fájlok gyorsítótárazása 1 hónapig
    ExpiresByType application/javascript "access plus 1 month"
    ExpiresByType text/javascript "access plus 1 month"
  
    # Képek gyorsítótárazása 1 évig
    ExpiresByType image/webp "access plus 1 year"
    ExpiresByType image/png "access plus 1 year"
    ExpiresByType image/jpg "access plus 1 year"
    ExpiresByType image/jpeg "access plus 1 year"
    ExpiresByType image/gif "access plus 1 year"
  
    # Videó gyorsítótárazása 1 évig
    ExpiresByType video/mp4 "access plus 1 year"
  
    # Betűkészletek gyorsítótárazása 1 évig
    ExpiresByType font/ttf "access plus 1 year"
    ExpiresByType font/woff "access plus 1 year"
    ExpiresByType font/woff2 "access plus 1 year"
    ExpiresByType application/font-woff "access plus 1 year"
    ExpiresByType application/font-woff2 "access plus 1 year"
  
    # HTML és a többi dinamikus tartalom nem gyorsítótárazandó
    ExpiresByType text/html "access plus 0 seconds"
    ExpiresDefault "access plus 0 seconds"
  </IfModule>
  
  <IfModule mod_headers.c>
    # A gyorsítótár-szabályok megerősítése, még ha a Cookie vagy ETag változik is
    <filesMatch "\.(js|css|xml|gz|webp|jpg|jpeg|png|gif|mp4|woff|woff2|ttf)$">
      Header append Cache-Control "public"
    </filesMatch>
  
    # Proaktív erőforrás-töltés beállítása (preload) a CSS és JS fájloknál
    Header set Link "</css/syliesgoko.css>; rel=preload; as=style"
    Header set Link "</script.js>; rel=preload; as=script"
  </IfModule>

